install-vpopmailbinPROGRAMS: $(vpopmailbin_PROGRAMS)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(vpopmailbindir)
	@list='$(vpopmailbin_PROGRAMS)'; for p in $$list; do \
	  p1=`echo $$p|sed 's/$(EXEEXT)$$//'`; \
	  if test -f $$p \
	     || test -f $$p1 \
	  ; then \
	    f=`echo "$$p1" | sed 's,^.*/,,;$(transform);s/$$/$(EXEEXT)/'`; \
	   echo " $(INSTALL_PROGRAM_ENV) $(LIBTOOL) --mode=install $(vpopmailbinPROGRAMS_INSTALL) $$p $(DESTDIR)$(vpopmailbindir)/$$f"; \
	   $(INSTALL_PROGRAM_ENV) $(LIBTOOL) --mode=install $(vpopmailbinPROGRAMS_INSTALL) $$p $(DESTDIR)$(vpopmailbindir)/$$f; \
	  else :; fi; \
	done

