	@$(NORMAL_UNINSTALL)
	@list='$(vpopmailbin_PROGRAMS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's,^.*/,,;s/$(EXEEXT)$$//;$(transform);s/$$/$(EXEEXT)/'`; \
	  echo " rm -f $(DESTDIR)$(vpopmailbindir)/$$f"; \
	  rm -f $(DESTDIR)$(vpopmailbindir)/$$f; \
	done

