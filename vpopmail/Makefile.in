	@if test ! -d cdb/makefile; then \
	  cp cdb/Makefile cdb/makefile; \
	  echo "distdir:" >> cdb/makefile; \
	  echo "	@echo 'nothing to do in cdb'" >> cdb/makefile; \
	  echo "" >> cdb/makefile; \
	fi
