  #include <stdio.h>
  #include <pwd.h>
  #include <stdlib.h>

  int main() {
    struct passwd *pw;
    char tmpbuf[100];
    FILE *f;

        unlink("vpopmail.dir");
        unlink("vpopmail.uid");
        unlink("vpopmail.gid");

        if ( (f=fopen("vpopusername","r+"))==NULL) {
                printf("No vpopmail user found.\n");
                printf("Please add the vchkpw group and\n");
                printf("vpopmail user. Then run configure again\n");
