then
   { $as_echo "$as_me:$LINENO: WARNING: ===                                                       ===" >&5
$as_echo "$as_me: WARNING: ===                                                       ===" >&2;}
   { $as_echo "$as_me:$LINENO: WARNING: ===               pthreads not found                      ===" >&5
$as_echo "$as_me: WARNING: ===               pthreads not found                      ===" >&2;}
   { $as_echo "$as_me:$LINENO: WARNING: === You will be unable to build the vpopmail usage daemon ===" >&5
$as_echo "$as_me: WARNING: === You will be unable to build the vpopmail usage daemon ===" >&2;}
   { $as_echo "$as_me:$LINENO: WARNING: ===                                                       ===" >&5
$as_echo "$as_me: WARNING: ===                                                       ===" >&2;}
   vusagedbuild=""

fi

#
# Check for libev requirements
#


for ac_header in ev.h
do
as_ac_Header=`$as_echo "ac_cv_header_$ac_header" | $as_tr_sh`
if { as_var=$as_ac_Header; eval "test \"\${$as_var+set}\" = set"; }; then
  { $as_echo "$as_me:$LINENO: checking for $ac_header" >&5
$as_echo_n "checking for $ac_header... " >&6; }
